                                            <TextBox
                                                materialDesign:HintAssist.Hint="更改昵称"  
                                                materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding Change_Name, UpdateSourceTrigger=PropertyChanged}"/>
                                            <TextBox
                                                materialDesign:HintAssist.Hint="查询金额"  
                                                materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding View_Balances , UpdateSourceTrigger=PropertyChanged}"/>
                                        </WrapPanel>
                                    </Grid>
                                </TabItem>
                                <TabItem  Header="卡片指令">
