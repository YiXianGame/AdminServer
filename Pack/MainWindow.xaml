                    <Viewbox>
                        <Grid DataContext="{Binding Path=Author, UpdateSourceTrigger=PropertyChanged}" >
                            <Border BorderThickness="0.1" Background="White" CornerRadius="30" >
                                <Border.Effect>
                                    <DropShadowEffect Color="Blue" BlurRadius="10" ShadowDepth="0" Opacity="1" />
                                </Border.Effect>
                            </Border>
                            <Label  Margin="181,42,531,765" HorizontalContentAlignment="Center"  Content="{Binding Path=NickName}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" FontFamily="楷体" FontSize="100" FontWeight="Bold"  />
                            <TextBox Width="1200" Height="350" Style="{StaticResource  MaterialDesignOutlinedTextFieldTextBox}"  Text="{Binding Path=Information}" FontSize="50" VerticalAlignment="Stretch" Margin="95,530,103,60" HorizontalAlignment="Stretch" FontFamily="楷体"  TextWrapping="Wrap" FontWeight="Bold" materialDesign:HintAssist.Hint="个性签名" />
                            <Label Content="已创作的卡牌数量:"  Margin="106,312,830,564" FontSize="50" />
                            <Label Content="{Binding Path=Create_num}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Center"  Margin="538,407,616,484" FontSize="50" RenderTransformOrigin="0.512,0.462" Foreground="Crimson"/>
