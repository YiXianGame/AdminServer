                                        materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding Upgrade_Card_Coast, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,50,0"/>
                                            <TextBox 
                                                materialDesign:HintAssist.Hint="出售金额"  
                                        materialDesign:HintAssist.FloatingScale="0.35" 
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding Sell_Card_Coast, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,50,0"/>
                                            <TextBox 
                                                materialDesign:HintAssist.Hint="战备上限"   
                                        materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding Battle_Max, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,50,0"/>
                                            <TextBox 
                                                materialDesign:HintAssist.Hint="仓库上限"   
                                        materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding Repository_Max, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,50,0"/>
                                            <TextBox 
                                                materialDesign:HintAssist.Hint="QQ主人"   
                                                materialDesign:HintAssist.FloatingScale="0.35"
