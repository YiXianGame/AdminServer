                                        materialDesign:HintAssist.FloatingScale="0.35"
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding Adventure_Probability, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,50,0"/>
                                            <TextBox 
                                        materialDesign:HintAssist.Hint="血量上限"   
                                        materialDesign:HintAssist.FloatingScale="0.35"
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding Room_Hp_Max, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,50,0"/>
                                            <TextBox 
                                        materialDesign:HintAssist.Hint="仙气上限"   
                                        materialDesign:HintAssist.FloatingScale="0.35"
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding Room_Mp_Max, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,50,0"/>
                                        </WrapPanel>
                                        <WrapPanel Margin="330,303,397,227">
                                            <StackPanel Orientation="Horizontal" Margin="50,0,40,0" Width="110" Height="35">
                                                <TextBlock Text="奇遇并发" Margin="0,8,5,9" FontFamily="楷体" FontWeight="Bold" FontSize="15"/>
                                                <ToggleButton Style="{StaticResource MaterialDesignSwitchToggleButton}" IsChecked="{Binding Adventure_Concurrent, UpdateSourceTrigger=PropertyChanged}" Height="Auto" Margin="0" Width="Auto"/>
                                            </StackPanel>
                                        </WrapPanel>
                                    </Grid>
                                </TabItem>
                                <TabItem  Header="仙域大陆" Background="{x:Null}" BorderBrush="{x:Null}" Margin="0">
                                    <Grid>
                                        <WrapPanel HorizontalAlignment="Left" Height="597" Margin="9,17,0,0" VerticalAlignment="Top" Width="1084"  Orientation="Horizontal">
                                            <TextBox x:Name="Mp"
                                        materialDesign:HintAssist.Hint="血上限"   
                                        materialDesign:HintAssist.FloatingScale="0.35"
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" Grid.Row="1" FontSize="45" Text="{Binding Map_Hp_Max, UpdateSourceTrigger=PropertyChanged}"/>
                                            <TextBox x:Name="Probability"
                                        materialDesign:HintAssist.Hint="地图人数"   
                                        materialDesign:HintAssist.FloatingScale="0.35"
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" Grid.Column="1" FontSize="45"  Text="{Binding Map_Players_Max, UpdateSourceTrigger=PropertyChanged}"/>
                                            <TextBox x:Name="Mp_Copy2"
                                        materialDesign:HintAssist.Hint="气上限"   
                                        materialDesign:HintAssist.FloatingScale="0.35"
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" Grid.Row="1" FontSize="45" Text="{Binding Map_Mp_Max, UpdateSourceTrigger=PropertyChanged}" Grid.Column="2"/>
                                            <TextBox x:Name="Probability_Copy"
                                        materialDesign:HintAssist.Hint="入场费用"   
                                        materialDesign:HintAssist.FloatingScale="0.35"
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" Grid.Column="1" FontSize="45"  Text="{Binding Map_Enter_Coast, UpdateSourceTrigger=PropertyChanged}" Grid.Row="2"/>
                                            <TextBox x:Name="Probability_Copy1"
                                                materialDesign:HintAssist.Hint="入场费用"   
                                                materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45"  Text="{Binding Map_Enter_Coast, UpdateSourceTrigger=PropertyChanged}"/>

                                            <TextBox x:Name="Probability_Copy2"
                                                materialDesign:HintAssist.Hint="奇遇资源"   
                                                materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45"  Text="{Binding Map_Resources_Adventure , UpdateSourceTrigger=PropertyChanged}"/>
                                            <StackPanel Orientation="Horizontal" Margin="50,0,0,0" Width="80" Height="30">
                                                <TextBlock Text="开启" Margin="0,8,5,9" FontFamily="楷体" FontWeight="Bold" FontSize="13"/>
                                                <ToggleButton Style="{StaticResource MaterialDesignSwitchToggleButton}" IsChecked="{Binding Map_Enable, UpdateSourceTrigger=PropertyChanged}" Height="Auto" Margin="0" Width="Auto"/>
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" Margin="50,0,0,0" Width="105" Height="30">
                                                <TextBlock Text="入场费用" Margin="0,8,5,9" FontFamily="楷体" FontWeight="Bold" FontSize="13"/>
                                                <ToggleButton Style="{StaticResource MaterialDesignSwitchToggleButton}" IsChecked="{Binding Map_Enter_Is_Coast, UpdateSourceTrigger=PropertyChanged}" Height="Auto" Margin="0" Width="Auto"/>
                                            </StackPanel>
                                            <TextBox x:Name="Probability_Copy3"
                                                materialDesign:HintAssist.Hint="技能卡资源"   
                                                materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45"  Text="{Binding Map_Resources_SkillCard , UpdateSourceTrigger=PropertyChanged}"/>
                                            <TextBox x:Name="Probability_Copy4"
                                                materialDesign:HintAssist.Hint="状态资源"   
                                                materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45"  Text="{Binding Map_Resources_State , UpdateSourceTrigger=PropertyChanged}"/>
                                            <TextBox x:Name="Probability_Copy5"
                                                materialDesign:HintAssist.Hint="野怪资源"   
                                                materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45"  Text="{Binding Map_Resources_Monster , UpdateSourceTrigger=PropertyChanged}"/>
                                            <TextBox x:Name="Probability_Copy6"
                                                materialDesign:HintAssist.Hint="地图大小"   
                                                materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45"  Text="{Binding Map_Size , UpdateSourceTrigger=PropertyChanged}"/>
                                            <TextBox x:Name="Probability_Copy7"
                                                materialDesign:HintAssist.Hint="传送费用"   
                                                materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45"  Text="{Binding TP_Coast, UpdateSourceTrigger=PropertyChanged}"/>
                                        </WrapPanel>
                                    </Grid>
                                </TabItem>
                            </TabControl>
                            <Button Style="{StaticResource MaterialDesignRaisedButton}" Content="存" FontSize="50" Margin="1175,37,39,580" Background="#FF68DBEE" Click="Button_Click" Height="Auto"></Button>
                        </Grid>
                    </Viewbox>
                </TabItem>
                <TabItem x:Name="Menu_Command" Header="指令设置">
                    <Viewbox>
                        <Grid>
                            <Border BorderThickness="0.1" Margin="10" Background="{StaticResource ResourceKey=MaterialDesignPaper}" CornerRadius="30" BorderBrush="{StaticResource AccentBaseColorBrush}" >
                                <Border.Effect>
                                    <DropShadowEffect Color="Blue" BlurRadius="10" ShadowDepth="0" Opacity="1" />
                                </Border.Effect>
                            </Border>
                            <TabControl Margin="15,12,20,14" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}">
                                <TabItem  Header="游戏指令" Height="22" VerticalAlignment="Top">
                                    <Grid Margin="0,0,0,0" Height="478"  VerticalAlignment="Top"  HorizontalAlignment="Left" Width="892">
                                        <WrapPanel Height="460" Margin="0,0,0,0" VerticalAlignment="Top"  Orientation="Horizontal" HorizontalAlignment="Left" Width="900">
                                            <TextBox 
                                            materialDesign:HintAssist.Hint="创建用户"  
                                            materialDesign:HintAssist.FloatingScale="0.35"
