                                                <ToggleButton Margin="0,0,0,5" HorizontalAlignment="Center" Style="{StaticResource MaterialDesignSwitchToggleButton}" IsChecked="{Binding Can_Buy , UpdateSourceTrigger=PropertyChanged}" />
                                                <TextBlock HorizontalAlignment="Center" Text="是否购买"  FontFamily="楷体" FontWeight="Bold" FontSize="20"/>
                                            </StackPanel>
                                            <StackPanel Orientation="Vertical"  Width="105" Height="50">
