                                            <TextBox 
                                            materialDesign:HintAssist.Hint="玩法介绍"  
                                            materialDesign:HintAssist.FloatingScale="0.35"
                                            Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45"  Text="{Binding GameMenu, UpdateSourceTrigger=PropertyChanged}" Grid.Column="2"/>
                                            <TextBox 
                                            materialDesign:HintAssist.Hint="全局统计" 
                                            materialDesign:HintAssist.FloatingScale="0.35"
                                            Style="{StaticResource  MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45"  Text="{Binding Game_Total, UpdateSourceTrigger=PropertyChanged}" Grid.Column="3"/>
                                            <TextBox
                                            materialDesign:HintAssist.Hint="个人信息" 
                                            materialDesign:HintAssist.FloatingScale="0.35"
                                            Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" Grid.Row="1" FontSize="45" Text="{Binding Personal_Information, UpdateSourceTrigger=PropertyChanged}" Grid.Column="1"/>
                                            <TextBox
                                            materialDesign:HintAssist.Hint="版本详情"  
                                            materialDesign:HintAssist.FloatingScale="0.35"
                                            Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" Grid.Row="1" FontSize="45" Text="{Binding Version_Information, UpdateSourceTrigger=PropertyChanged}" Grid.Column="3"/>
                                            <TextBox
                                                materialDesign:HintAssist.Hint="查看手牌"  
                                                materialDesign:HintAssist.FloatingScale="0.35"
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="0,0,40,0" Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding View_Hand , UpdateSourceTrigger=PropertyChanged}"/>
                                            <TextBox
                                                materialDesign:HintAssist.Hint="充值仙域币"  
                                                materialDesign:HintAssist.FloatingScale="0.35"
