                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="45" Text="{Binding Buy_Card_Coast, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,50,0"/>
                                            <TextBox 
                                                materialDesign:HintAssist.Hint="抽卡金额"   
                                        materialDesign:HintAssist.FloatingScale="0.35"
