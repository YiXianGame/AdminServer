                            <Label Content="已更新的卡牌数量:"   FontSize="50" Margin="106,402,841,480"/>
                            <Label Content="{Binding Path=Upgrade_num}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Center" Margin="536,316,614,572" FontSize="50" RenderTransformOrigin="0.512,0.462" Foreground="Crimson"/>
                            <Label Content="张"  FontSize="50" Margin="763,406,543,472"/>
                            <Label Content="QQ号:"  FontSize="50" Margin="106,233,1106,652"/>
                            <TextBox Style="{StaticResource   MaterialDesignDataGridComboBoxEditableTextBox}" Text="{Binding Path=QQ}" HorizontalContentAlignment="Stretch" Margin="260,239,229,652" VerticalContentAlignment="Stretch" FontSize="50"/>
                            <Button Content="存" Style="{StaticResource MaterialDesignFloatingActionDarkButton}" Background="LightSeaGreen" HorizontalAlignment="Left" Height="101" Margin="1170,55,0,0" VerticalAlignment="Top" Width="106" FontSize="40" Click="Button_Click_3"/>
                            <Button Content="刷" Style="{StaticResource MaterialDesignFloatingActionDarkButton}" Background="#FF36CDF9" HorizontalAlignment="Left" Height="101" Margin="1031,55,0,0" VerticalAlignment="Top" Width="106" FontSize="36" Click="Button_Click_4"/>
                        </Grid>
                    </Viewbox>
                </TabItem>
                <TabItem x:Name="Menu_Skill_Cards" Header="技能设置">
                    <Grid>
                        <Element:CardPanle x:Name="CardPanle"/>
                    </Grid>
                </TabItem>
                <TabItem x:Name="Menu_Adventure_Cards" Header="奇遇设置" >
                    <Grid>
                        <Element:AdventurePanle x:Name="AdventurePanle"/>
                    </Grid>
                </TabItem>
                <TabItem x:Name="Menu_GameControl" Header="仙战配置" >
